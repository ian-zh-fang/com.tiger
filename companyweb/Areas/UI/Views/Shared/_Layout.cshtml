<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title> 杭州塔格科技有限公司 - @ViewBag.Title</title>

    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <link rel="stylesheet" href="@Url.Content("~/Areas/UI/css/style.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Areas/UI/css/settings.css")" media="screen" />

    <link rel="shortcut icon" href="@Url.Content("~/Areas/UI/images/favicon.ico")">
    <link rel="apple-touch-icon" href="@Url.Content("~/Areas/UI/images/apple-touch-icon.png")">
    <link rel="apple-touch-icon" sizes="72x72" href="@Url.Content("~/Areas/UI/images/apple-touch-icon-72x72.png")">
    <link rel="apple-touch-icon" sizes="114x114" href="@Url.Content("~/Areas/UI/images/apple-touch-icon-114x114.png")">
    @RenderSection("styles", required:false)

    <script src="@Url.Content("~/Scripts/globalfuncs.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/UI/js/jquery-1.8.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/UI/js/jquery.easing.1.3.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/UI/js/modernizr.custom.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/UI/js/jquery-ui-1.10.1.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/UI/js/custom.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/UI/js/jquery.themepunch.plugins.min.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Areas/UI/js/jquery.themepunch.revolution.min.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Areas/UI/js/jquery.prettyPhoto.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Areas/UI/js/jquery.flexisel.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Areas/UI/js/jquery.meanmenu.js")" type="text/javascript" ></script>
    @RenderSection("scripts", required: false)

</head>
<body>

    <header class="header">
        <div class="container">
            <div class="logos columns">
                <h1 class="logo"><a href="index.html"><img src="@Url.Content("~/Areas/UI/images/logo.png")" alt="EVOLVE" /></a></h1>
            </div>

            <div class="twelve columns">
                <nav class="main_menu">

                    <ul>
                        <li class="current_page_item">
                            <a href="@Url.Action("Index")">
                                主页
                                <div class="sub">Home</div>
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("Aboutus")">
                                关于我们
                                <div class="sub">
                                    About us
                                </div>
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("ProductAndService")">
                                产品及服务
                                <div class="sub">Products &amp; Services</div>
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("SolutionAndCase")">
                                解决方案及案例
                                <div class="sub">
                                    Solutions &amp; Cases
                                </div>
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("Contact")">
                                联系我们
                                <div class="sub">Contact us</div>
                            </a>
                        </li>
                    </ul>

                </nav>
            </div>
            <div class="clearfix"></div>
        </div>
    </header>

    @RenderBody()

    <footer class="footer">
        <div class="container">
            <div class="footer-top clearfix">
                <div class="four columns">
                    <h3>联系方式</h3>

                </div>
                <table style="width:100%; border-width:0px;">
                    <tr>
                        <td><i class="fa fa-map-marker"></i>&nbsp;&nbsp;&nbsp;<span>320010. 杭州市, 西湖区, 翠苑街道, 文三路, No.259. 昌地·火炬大厦, 一号楼, 1301</span></td>
                        <td><i class="fa fa-envelope"></i>&nbsp;&nbsp;&nbsp;<a class="link" href="mailto:tiger@tigerhz.com">tiger@tigerhz.com</a></td>
                    </tr>
                    <tr>
                        <td><i class="fa fa-phone"></i>&nbsp;&nbsp;&nbsp;<span>+86 (0571) 8649-9866</span></td>
                        <td>
                            <i class="fa fa-clock-o"></i>&nbsp;&nbsp;&nbsp;<span>
                                Monday-Friday: 9:<sup>00</sup> - 18:<sup>00</sup>&nbsp;&nbsp;&nbsp;
                                Saturday, Sunday: closed
                            </span>
                        </td>
                    </tr>
                </table>
            </div>
        </div>

        <div class="container"></div>
    </footer>

    <script type="text/javascript">
        jQuery('header nav').meanmenu();
    </script>
</body>
</html>
